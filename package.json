{
  "name": "zero-company-app",
  "version": "0.1.0",
  "private": true,
  "description": "Zero Company App",
  "keywords": [],
  "homepage": "https://github.com/iliubinskii/zero-company-app",
  "bugs": {
    "url": "https://github.com/iliubinskii/zero-company-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iliubinskii/zero-company-app"
  },
  "license": "Proprietary",
  "author": "Ilia Liubinskii <ilia.liubinskii@gmail.com> (https://github.com/iliubinskii)",
  "type": "module",
  "scripts": {
    "build": "next build",
    "contributors": "python -m gitfame --bytype --excl \"\\.(ico|json|svg)$\" ./src >./CONTRIBUTORS.md",
    "deploy-preview": "vercel",
    "deploy-prod": "vercel --prod",
    "dev": "next dev -p 3100",
    "docker:build": "docker build -t zero-company-app .",
    "docker:start": "docker run --env-file .env -p 3100 zero-company-app",
    "format": "prettier --write \"**/*.{css,html,json,less,postcss,scss}\"",
    "install-deps": "npm install --no-audit --save-prod --strict-peer-deps",
    "install-deps-clean": "npm install-clean --no-audit --save-prod --strict-peer-deps",
    "install-dev-deps": "npm install --no-audit --save-dev --strict-peer-deps",
    "install-dev-deps-clean": "npm install-clean --no-audit --save-dev --strict-peer-deps",
    "lint": "eslint --fix --max-warnings=0 **/*.{cjs,js,jsx,mjs,ts,tsx}",
    "lint-commit-msg": "commitlint --edit",
    "lint-markdown": "markdownlint *.md",
    "lint-no-fix": "eslint --max-warnings=0 **/*.{cjs,js,jsx,mjs,ts,tsx}",
    "lint-profile": "cross-env TIMING=1 eslint --max-warnings=0 **/*.{cjs,js,jsx,mjs,ts,tsx}",
    "lint-staged-files": "lint-staged",
    "lint-style": "stylelint --fix --max-warnings=0 src/**/*.{css,less,postcss,scss}",
    "lint-style-no-fix": "stylelint --max-warnings=0 src/**/*.{css,less,postcss,scss}",
    "ncu-check": "ncu --dep=dev,peer,prod",
    "ncu-upgrade": "ncu --dep=dev,peer,prod --upgrade && npm run install-deps",
    "prepare": "husky",
    "preversion": "run-s preversion:install preversion:fix preversion:lint preversion:test preversion:add",
    "preversion:install": "npm run install-deps",
    "preversion:fix": "run-p contributors format lint-markdown",
    "preversion:lint": "run-p lint-no-fix lint-style-no-fix type-check",
    "preversion:test": "run-p test test-e2e",
    "preversion:add": "git add .",
    "start": "next start -p 3100",
    "test": "cf coverage/lcov-report && jest --coverage",
    "test-e2e": "playwright test",
    "type-check": "tsc --incremental false --noEmit",
    "update": "npm update --no-audit --strict-peer-deps"
  },
  "dependencies": {
    "@docuseal/react": "^1",
    "@headlessui/react": "^2",
    "@reduxjs/toolkit": "^2",
    "@types/dot-object": "^2",
    "@types/gravatar": "^1",
    "@types/lodash": "^4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "date-fns": "^3",
    "dot-object": "^2",
    "framer-motion": "^11",
    "gravatar": "^1",
    "http-status-codes": "^2",
    "lodash": "^4",
    "next": "^14",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14",
    "react-icons": "^5",
    "react-redux": "^9",
    "react-spinners": "^0.14",
    "react-world-flags": "^1",
    "redux-thunk": "^3",
    "styled-components": "^6",
    "tailwind-styled-components": "^2",
    "ts-toolbelt": "^9",
    "zod": "^3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19",
    "@commitlint/config-conventional": "^19",
    "@cspell/eslint-plugin": "^8",
    "@happy-dom/jest-environment": "^14",
    "@jest/globals": "^29",
    "@next/eslint-plugin-next": "^14",
    "@playwright/test": "^1",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^16",
    "@types/eslint": "^8",
    "@types/jest": "^29",
    "@types/lint-staged": "^13",
    "@types/node": "^20",
    "@types/stylelint": "^14",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "autoprefixer": "^10",
    "clear-folder": "^5",
    "cross-env": "^7",
    "daisyui": "^4",
    "dotenv": "^16",
    "envalid": "^8",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-config-standard": "^17",
    "eslint-config-strict": "^14",
    "eslint-plugin-es": "^4",
    "eslint-plugin-escompat": "^3",
    "eslint-plugin-eslint-comments": "^3",
    "eslint-plugin-etc": "^2",
    "eslint-plugin-github": "^5",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest-extended": "^2",
    "eslint-plugin-jsdoc": "^48",
    "eslint-plugin-misc": "^3",
    "eslint-plugin-n": "^16",
    "eslint-plugin-no-type-assertion": "^1",
    "eslint-plugin-no-use-extend-native": "^0.5",
    "eslint-plugin-no-useless-assign": "^1",
    "eslint-plugin-only-warn": "^1",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-regexp": "^2",
    "eslint-plugin-security": "^3",
    "eslint-plugin-sonarjs": "^0.25",
    "eslint-plugin-sort": "^3",
    "eslint-plugin-sort-annotation": "^1",
    "eslint-plugin-sort-imports-requires": "^1",
    "eslint-plugin-typescript-sort-keys": "^3",
    "eslint-plugin-unicorn": "^54",
    "eslint-plugin-unused-imports": "^3",
    "flowbite": "^2",
    "flowbite-react": "^0.10",
    "husky": "^9",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "jest-extended": "^4",
    "lint-staged": "^15",
    "local-ssl-proxy": "^2",
    "markdownlint-cli": "^0.41",
    "npm-check-updates": "^16",
    "npm-run-all": "^4",
    "postcss": "^8",
    "postcss-import": "^16",
    "prettier": "^3",
    "prettier-plugin-sort-json": "^4",
    "stylelint": "^16",
    "stylelint-config-property-sort-order-smacss": "^10",
    "stylelint-config-standard": "^36",
    "stylelint-config-standard-scss": "^13",
    "stylelint-config-tailwindcss": "^0.0",
    "stylelint-no-unsupported-browser-features": "^8",
    "stylelint-value-no-unknown-custom-properties": "^6",
    "tailwindcss": "^3",
    "ts-jest": "^29",
    "typescript": "^5",
    "vercel": "^34"
  }
}
